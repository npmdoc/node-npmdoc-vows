{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "vows",
    "description": "api documentation for [vows (v0.8.1)](https://github.com/cloudhead/vows) [![npm package](https://img.shields.io/npm/v/npmdoc-vows.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-vows) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-vows.svg)](https://travis-ci.org/npmdoc/node-npmdoc-vows)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha",
        "vows": "*"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-vows",
    "keywords": [
        "documentation",
        "vows"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_vows.js",
    "name": "npmdoc-vows",
    "nameAlias": "npmdoc_vows",
    "nameOriginal": "npmdoc-vows",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-vows.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.2"
}
